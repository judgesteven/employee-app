{
  "version": 2,
  "name": "employee-step-up-app",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_GAMELAYER_API_KEY": "@gamelayer_api_key",
    "REACT_APP_GAMELAYER_API_URL": "https://api.gamelayer.co"
  }
}
